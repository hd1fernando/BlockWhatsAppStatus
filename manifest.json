{
    "manifest_version": 2,
    "name": "Block Whats App Status",
    "description": "Block wpp status",
    "version": "0.1.0",
    "icons": {
        "64": "icons/wpp-block.png"
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "clipboardWrite"
    ],
    "background": {
        "scripts": [
            "app.js"
        ]
    },
    "content_scripts": [],
    "browser_action": {
        "default_icon": {
            "64": "icons/wpp-block.png"
        }
    }
}